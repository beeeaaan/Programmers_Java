import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int c = sc.nextInt();

        // int b_square = c - a;  // 오류 부분
        int b_square = (c * c) - (a * a);  // 수정

        System.out.println(b_square);
    }
}

// 입력 #1
// 3
// 5
// 출력 #1
// 16

// 입력 #1
// 9
// 10
// 출력 #2
// 19
