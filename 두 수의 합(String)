import java.math.BigInteger;

class Solution {
    public String solution(String a, String b) {
        //  BigInteger : long 범위를 벗어나는 수
        BigInteger A = new BigInteger(a);
        BigInteger B = new BigInteger(b);
        BigInteger answer;
        
        answer = A.add(B);
        
        return answer.toString();  // 문자열로 변환
    }
}
