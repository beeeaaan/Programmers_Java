import java.util.*;

class Solution {
    public List solution(String[] todo_list, boolean[] finished) {
        List<String> answer = new ArrayList<>();
        
        // true : 할 일 마침   false : 할 일 못 마침
        for(int i = 0; i < todo_list.length; i++) {
            if(finished[i] == false) {
                answer.add(todo_list[i]);
            }
        }
        return answer;
    }
}
