import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        String msg = // 빈 칸
                    "Spring is beginning";
        int val1 = // 빈 칸
                    3;
        String val2 = // 빈 칸
                      "3";

        System.out.println(msg);
        System.out.println(val1 + 10);
        System.out.println(val2 + "10");
    }
}

