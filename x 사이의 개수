class Solution {
    public int[] solution(String myString) {
        String[] splArr = myString.split("x", myString.length());  // "x" 기준으로 자르기
        int[] answer = new int[splArr.length];
        
        // 갯수 세기
        for (int i = 0; i < splArr.length; i++) {
            answer[i] = splArr[i].length();
        }
        return answer;
    }
}
