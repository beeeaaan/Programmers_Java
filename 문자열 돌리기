import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        
        for(int i = 0; i < a.length(); i++) {
            System.out.println(a.substring(i, i + 1));  // 한 줄씩 출력하기 위해 println 사용
        }
    }
}
