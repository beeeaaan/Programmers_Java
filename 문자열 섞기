class Solution {
    public String solution(String str1, String str2) {
        String answer = "";
        
        for(int i = 0; i < str1.length(); i++){  // 반복문 통해 이어 붙이기
            answer += str1.charAt(i);  // 1. str1 출력
            answer += str2.charAt(i);  // 2. str2 출력
        }
        return answer;
    }
}
