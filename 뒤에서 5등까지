import java.util.Arrays;

class Solution {
    public int[] solution(int[] num_list) {
        int[] answer = new int [5];
        
        Arrays.sort(num_list);  // 오름차순 정렬
        // Array.copyOfRange(배열, 시작, 끝) : 자신이 복사하고자 하는 배열의 범위만 지정해서 복사
        answer = Arrays.copyOfRange(num_list, 0, 5);
        return answer;
    }
}
