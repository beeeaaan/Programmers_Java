import java.util.*;

class Solution {
    public List solution(int[] num_list, int n) {
        // ArrayList : 크기 가변적으로 변하는 선형 리스트
        List<Integer> answer = new ArrayList<> ();
        
        for(int i = 0; i < num_list.length; i += n) {
            // add : List에 값 하나씩 추가
            answer.add(num_list[i]);
        }
        return answer;
    }
}
